{
  "name": "svelte-themes-preprocess",
  "version": "1.0.5",
  "description": "Add support flexible themes for svelte components",
  "author": "Nikolay Makhonin",
  "homepage": "https://github.com/NikolayMakhonin/svelte-themes-preprocess",
  "bugs": {
    "url": "https://github.com/NikolayMakhonin/svelte-themes-preprocess/issues"
  },
  "license": "CC0-1.0",
  "keywords": [
    "svelte",
    "preprocess",
    "preprocessor",
    "style",
    "styles",
    "stylesheet",
    "css",
    "scss",
    "less",
    "styl",
    "stylus",
    "sass",
    "theme",
    "themes",
    "theming",
    "cascading"
  ],
  "files": [
    "dist/**",
    "env/register-*"
  ],
  "main": "dist/js/main/node/main.js",
  "jsnext:main": "dist/mjs/main/node/main.js",
  "module": "dist/mjs/main/node/main.js",
  "engines": {
    "node": ">= 8.6.0"
  },
  "scripts": {
    "clean": "shx rm -rf {dist,tmp,*.log}",
    "precommit": "npm run test:all:coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:all",
    "build:mjs": "shx rm -rf dist/mjs/ && shx mkdir -p dist/mjs/ && shx cp -rf src/test dist/mjs/ && babel src --out-dir dist/mjs/ --no-babelrc --config-file ./.babelrc-mjs.js",
    "build:js": "shx rm -rf dist/js/ && shx mkdir -p dist/js/ && shx cp -rf src/test dist/js/  && babel src --out-dir dist/js/ --no-babelrc --config-file ./.babelrc-js.js",
    "build": "npm run build:mjs && npm run build:js",
    "lint": "eslint --plugin markdown --ext js,md .",
    "test:mocha:src": "mocha --opts ./env/mocha.babel.opts ./src/test/tests/{node,common}/**/*.*",
    "test:mocha:mjs": "mocha --opts ./env/mocha.babel.opts ./dist/mjs/test/tests/{node,common}/**/*.*",
    "test:mocha:js": "mocha --opts ./env/mocha.babel.opts ./dist/js/test/tests/{node,common}/**/*.*",
    "test:karma": "karma start env/karma.conf.tests.js",
    "test": "npm run test:mocha:mjs && npm run test:mocha:js",
    "test:all": "npm run lint && npm run clean && npm run test:mocha:src && npm run build && npm run test:mocha:mjs && npm run test:mocha:js",
    "test:all:coverage": "npm run lint && npm run clean && npm run build && npm run coverage && nyc check-coverage --report-dir tmp/coverage/lcov --lines 100 --functions 100 --branches 100",
    "watch:mocha": "mocha --opts ./env/mocha.babel.opts ./src/test/tests/{node,common}/**/*.* --watch",
    "perf:mocha": "mocha --opts ./env/mocha.babel.opts ./src/test/performance/{node,common}/**/*.*",
    "watch:karma": "karma start env/karma.conf.tests.js --auto-watch",
    "perf:karma": "karma start env/karma.conf.performance.js",
    "test:coverage": "npm run test:mocha:src && npm run test:mocha:mjs && npm run test:mocha:js",
    "coverage:mocha": "nyc npm run test:coverage",
    "coverage:karma": "karma start env/karma.conf.coverage.js",
    "coverage": "npm run coverage:mocha && npm run coverage:karma && npm run coverage:merge",
    "coverage:ci": "npm run coverage:mocha && npm run ci:karma && npm run coverage:merge",
    "coverage:merge": "istanbul-combine -d tmp/coverage/lcov -p summary -r lcov tmp/coverage/json/**/*.json",
    "ci:karma": "karma start env/karma.conf.ci.js",
    "ci": "npm run lint && npm run coverage:ci && ncat tmp/coverage/lcov/**/lcov.info --banner \"\" | coveralls"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@metahub/karma-rollup-preprocessor": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.3.0",
    "eslint-config-pro": "0.0.4",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "istanbul-combine": "^0.3.0",
    "js-beautify": "^1.10.2",
    "karma": "^4.3.0",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-detect-browsers": "^2.3.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-rollup-preprocessor": "^7.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-safaritechpreview-launcher": "^2.0.2",
    "less": "3.9.0",
    "mocha": "^5.2.0",
    "ncat": "^2.0.7",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "postcss": "^7.0.17",
    "postcss-global-nested": "^1.2.0",
    "postcss-import": "^12.0.1",
    "postcss-js-syntax": "^1.0.7",
    "postcss-nested": "^4.1.2",
    "require-from-memory": "^1.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-pluginutils": "^2.8.1",
    "shx": "^0.3.2",
    "stylus": "^0.54.7",
    "svelte": "^3.9.1",
    "svelte-preprocess": "^3.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "unresolve": "^1.0.2"
  }
}
